<body id="bimg2"></body>

<h1> List an Account</h1>

<form action="/account/create" method="POST">

<div>
    <br>Descrption:<br>
    <input type="text" name="accountDescription">
</div>

<div>
    <br>Price:<br>
    <input type="text" name="accountPrice">
</div>

<div>
    <br>Townhall:<br>
    <input type="text" name="accountTownhall">
</div>

<div>
    <br>Gems:<br>
    <input type="text" name="accountGems">
</div>

<div>
    <br>Sceneries:<br>
    <input type="text" name="accountSceneries">
</div>

<div>
    <br>Level:<br>
    <input type="text" name="accountLevel">
</div>

<div>
    <br>Builderhall:<br>
    <input type="text" name="accountBuilderhall">
</div>

<div>
    <br>Skins:<br>
    <input type="text" name="accountSkins">
</div>

<div>
    <br>Max:<br>
     <input id="max" type="radio" value=true name="accountMax">
    <label for="max">Yes</label>
    <input id="not-max" type="radio" value=false name="accountMax">
    <label for="not-max">No</label>
</div>

<div>
    <br>Images:<br>
    <input type="text" name="img">
</div>

<button>Save</button>

</form>